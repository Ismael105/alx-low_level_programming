#include <stdio.h>
/**
 * main - print numbers 0 to 100, Fizz for multiple 3,FizzBuzz for multiple of  * 3 & 5
 * return: Always 0 (Success)
 */
int main(void)
{
int i;
for (i = 0; i <= 100)
{
if (i % 3 == 0 && i % 5 == 0)
putchar("FizzBuzz");
else if (i % 3 == 0)
putchar("Fizz");
else
{
printf(i);
}
pritf(" ");
}
}
